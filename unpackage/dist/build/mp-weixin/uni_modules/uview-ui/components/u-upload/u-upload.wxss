view, scroll-view, swiper-item{display:flex;flex-direction:column;flex-shrink:0;flex-grow:0;flex-basis:auto;align-items:stretch;align-content:flex-start}.u-upload{display:flex;flex-direction:row}.u-upload__wrap{display:flex;flex-direction:row}.u-upload__wrap__preview{border-radius:2px;margin:0 8px 8px 0;position:relative;overflow:hidden;display:flex;flex-direction:row}.u-upload__wrap__preview__image{width:80px;height:80px}.u-upload__wrap__preview__other{background-color:#f2f2f2;flex:1;display:flex;flex-direction:column;justify-content:center;align-items:center}.u-upload__wrap__preview__other__text{font-size:11px;color:#909193;margin-top:2px}.u-upload__deletable{position:absolute;top:0;right:0;background-color:#373737;height:14px;width:14px;display:flex;flex-direction:row;border-bottom-left-radius:100px;align-items:center;justify-content:center;z-index:3}.u-upload__deletable__icon{position:absolute;-webkit-transform:scale(.7);transform:scale(.7);top:0;right:0}.u-upload__success{position:absolute;bottom:0;right:0;display:flex;flex-direction:row;border-top-color:transparent;border-left-color:transparent;border-bottom-color:#5ac725;border-right-color:#5ac725;border-width:9px;align-items:center;justify-content:center}.u-upload__success__icon{position:absolute;-webkit-transform:scale(.7);transform:scale(.7);bottom:-10px;right:-10px}.u-upload__status{position:absolute;top:0;bottom:0;left:0;right:0;background-color:rgba(0,0,0,.5);display:flex;flex-direction:column;align-items:center;justify-content:center}.u-upload__status__icon{position:relative;z-index:1}.u-upload__status__message{font-size:12px;color:#fff;margin-top:5px}.u-upload__button{display:flex;flex-direction:column;align-items:center;justify-content:center;width:80px;height:80px;background-color:#f4f5f7;border-radius:2px;margin:0 8px 8px 0;box-sizing:border-box}.u-upload__button__text{font-size:11px;color:#909193;margin-top:2px}.u-upload__button--hover{background-color:#f0f1f3}.u-upload__button--disabled{opacity:.